{# index.twig #}
{% extends "_includes/layout.twig" %}

{% block page %}
	<div class="container">
		<header>
	        <h1 class="fs30">hello world</h1>
	    </header>
	    <p>
	        This page is generated by Twig.js using the gulp-twig gulp plugin.
	    </p>

		<div id="gallery">
			<section >
				<div class="slick-nav">
					{% for value in index.list %}
			            <div>{{ value }}</div>
			        {% endfor %}
				</div>
			</section>

			<section>
				<div class="slick-for">
					{% for value in index.list %}
			            <div>{{ value }}</div>
			        {% endfor %}
				</div>
			</section>
		</div>
	</div>
{% endblock %}
