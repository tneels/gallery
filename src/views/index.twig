{# index.twig #}
{% extends "_includes/layout.twig" %}

{% block page %}
	<div class="container">
		<header>
	        <h1 class="fs30">hello world</h1>
	    </header>
	    <p>
	        This page is generated by Twig.js using the gulp-twig gulp plugin.
	    </p>

		<div class="slick-slider">
			<div>1</div>
			<div>2</div>
			<div>3</div>
		</div>
	    <ul>
	        {% for value in index.list %}
	            <li>{{ value }}</li>
	        {% endfor %}
	    </ul>
	</div>
{% endblock %}
